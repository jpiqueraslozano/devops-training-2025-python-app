    "version": "0.2.0",
    "configurations": [
        {
            "name": "App Debugger",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/app.py",
            "console": "integratedTerminal",
            "env": {
                "DB_USER": "root",
